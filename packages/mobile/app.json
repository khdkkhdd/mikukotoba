{
  "expo": {
    "name": "ミク言葉",
    "slug": "mikukotoba",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "scheme": "mikukotoba",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#F2F7F7"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mikukotoba.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#F2F7F7"
      },
      "package": "com.mikukotoba.app"
    },
    "plugins": [
      "expo-router",
      "expo-sqlite"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "36d89e2c-1d0e-4cc1-bfef-5b7e4946c400"
      }
    }
  }
}
